{
  "name": "alphasms-client",
  "version": "0.2.0",
  "description": "Lightweight TypeScript client for AlphaSMS JSON API with first-class support for Node.js and NestJS backends.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "lint": "eslint .",
    "test": "node -e \"console.log('ok')\"",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frontangel/alfasms-client"
  },
  "homepage": "https://github.com/frontangel/alfasms-client#readme",
  "bugs": {
    "url": "https://github.com/frontangel/alfasms-client/issues"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "alphasms",
    "alpha-sms",
    "sms",
    "sms-api",
    "sms-client",
    "api-client",
    "nodejs",
    "nestjs",
    "typescript",
    "json-api"
  ]
}
